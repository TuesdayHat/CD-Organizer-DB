{{#partial "content"}}

    <h1>CD Organizer</h1>

{{/partial}}

{{#partial "sorted"}}
    {{#if artists}}
        {{#each artists}}
            <p>Artist: </p>
            <h4>{{name}} <a href = "/artists/{{id}}">Details</a></h4>
            {{#each allCDs}}
                <p>{{title}}</p>
            {{/each}}
        {{/each}}
    {{else}}
        <p>...</p>
    {{/if}}
{{/partial}}

{{> layout.hbs}}